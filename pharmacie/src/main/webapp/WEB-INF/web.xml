  	<servlet-class>servlets.ArticleServlets</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ConnectionDB</servlet-name>
  	<url-pattern>/ConnectionDB</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ArticleServlets</servlet-name>
  	<url-pattern>/arti</url-pattern>
  </servlet-mapping>
